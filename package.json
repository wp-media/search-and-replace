{
  "name": "search-and-replace",
  "version": "3.1.0",
  "devDependencies": {
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-combine-mq": "^0.4.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-newer": "^1.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-uglify": "^2.0.0",
    "gulp-zip": "^4.0.0",
    "gulp-util": "^3.0.8"
  },
  "config": {
    "slug": "inpsyde-search-and-replace",
    "name": "Search & Replace",
    "scripts": {
      "src": "resources/js/",
      "dest": "assets/js/"
    },
    "styles": {
      "src": "resources/scss/",
      "dest": "assets/css/"
    },
    "images": {
      "src": "resources/img/",
      "dest": "assets/img/"
    },
    "svn_assets": {
      "src": "resources/svn-assets/",
      "dest": "svn-assets/"
    }
  },
  "browserslist": [
    "Chrome >= 30",
    "Firefox >= 30",
    "Android >= 4",
    "ChromeAndroid >= 4",
    "iOS >= 5",
    "safari >= 5"
  ]
}
